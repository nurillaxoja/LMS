<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import com.jfoenix.controls.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" fx:id="rootPane" prefHeight="333.0" prefWidth="400.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lms.addBook.AddBookController">
    <stylesheets>
        <URL value="@addbook.css" />
    </stylesheets>
   <children>
      <VBox layoutX="90.0" layoutY="14.0" prefHeight="400.0" prefWidth="400.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <JFXTextField fx:id="title" labelFloat="true" promptText="Book Title">
               <VBox.margin>
                  <Insets bottom="10.0" left="20.0" right="20.0" top="30.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXTextField fx:id="id" labelFloat="true" layoutX="10.0" layoutY="10.0" promptText="Book ID">
               <VBox.margin>
                  <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXTextField fx:id="author" labelFloat="true" layoutX="10.0" layoutY="41.0" promptText="Book Author">
               <VBox.margin>
                  <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXTextField fx:id="publisher" labelFloat="true" layoutX="10.0" layoutY="10.0" promptText="Publisher">
               <VBox.margin>
                  <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
               </VBox.margin>
            </JFXTextField>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <JFXButton fx:id="saveButton" onAction="#addBookButton" prefHeight="60.0" prefWidth="180.0" styleClass="addBook.css" stylesheets="@addbook.css" text="Save">
                     <HBox.margin>
                        <Insets left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </JFXButton>
                  <JFXButton fx:id="cancelButton" onAction="#cancelButton" prefHeight="60.0" prefWidth="180.0" styleClass="addBook.css" stylesheets="@addbook.css" text="Cancel">
                     <HBox.margin>
                        <Insets top="10.0" />
                     </HBox.margin>
                  </JFXButton>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
